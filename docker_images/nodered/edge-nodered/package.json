        "node-red-contrib-modbus": "5.43.0",
        "node-red-contrib-opcua": "0.2.289",
        "node-red-contrib-postgresql": "0.14.2",
        "node-red-contrib-prometheus-exporter": "1.0.5",
        "node-red-contrib-s7": "3.1.0",
        "node-red-contrib-s7comm": "1.1.6",
        "node-red-dashboard": "3.6.5",
        "node-red-docker": "file:",
